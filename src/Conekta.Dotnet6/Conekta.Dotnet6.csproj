<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>disable</Nullable>
    <AssemblyVersion>1.3.1</AssemblyVersion>
    <Authors>Brendan Alexander</Authors>
    <Company>Eventec</Company>
    <Description>Conekta Api wrapper for .NET 6 using the native System.Text.Json</Description>
    <PackageProjectUrl>https://github.com/brendanalexdr/Conekta.Dotnet6</PackageProjectUrl>
    <PackageReadmeFile>README-NUGET.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/brendanalexdr/Conekta.Dotnet6</RepositoryUrl>
    <PackageTags>conekta, brendan, alexander, payments</PackageTags>
    <Version>1.4.1</Version>
    <FileVersion>1.3.1</FileVersion>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
	<PackageReleaseNotes>Changed root namespace to ConektaDotnet6</PackageReleaseNotes>
	<PackageOutputPath>E:/Nuget-Local</PackageOutputPath>
	  <RootNamespace>ConektaDotnet6</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CSharpFunctionalExtensions" Version="2.31.0" />
    <PackageReference Include="RestSharp" Version="108.0.1" />
  </ItemGroup>

  <ItemGroup>
    <None Update="README-NUGET.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
